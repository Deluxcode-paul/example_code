<!DOCTYPE html>
<!--[if IE 7 ]><html class="ie no-js ie7"><![endif]-->
<!--[if IE 8 ]><html class="ie no-js ie8"><![endif]-->
<!--[if IE 9 ]><html class="ie no-js ie9"><![endif]-->
{% set html_classes = [ 'no-js' ] %}
<!--[if (gt IE 9)|!(IE) ]><html{{ html_attributes.addClass(html_classes) }}><![endif]-->
  <head>
    <meta name="format-detection" content="telephone=no">
    <head-placeholder token="{{ placeholder_token|raw }}">
    <title>{{ head_title|safe_join(' | ') }}</title>
    <css-placeholder token="{{ placeholder_token|raw }}">
    <js-placeholder token="{{ placeholder_token|raw }}">
  </head>
  <body{{ attributes }}>
    <a href="#main-content" class="visually-hidden focusable">{{ 'Skip to main content'|t }}</a>
    {{ page_top }}
    {{ page }}
    {{ page_bottom }}
    <js-bottom-placeholder token="{{ placeholder_token|raw }}">
  </body>
</html>
