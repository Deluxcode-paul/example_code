{%
  set classes = [
    dom_id ? 'js-view-dom-id-' ~ dom_id,
  ]
%}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {{ title }}
  {{ title_suffix }}
  {{ header }}
  {{ exposed }}
  {{ attachment_before }}
  {{ rows }}
  {{ empty }}
  <div class="pager_holder">
    {{ pager }}
  </div>
  {{ attachment_after }}
  {{ more }}
  {{ footer }}
  {{ feed_icons }}
</div>
